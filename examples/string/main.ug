fn main() {
    var src = "A B C";
    console.write(src);
    // => A B C

    var charArray = string.toChars(src);
    charArray[2] = '🍮';
    var dest = string.fromChars(charArray);

    console.write(dest);
    // => A 🍮 C

    var code = char.toNumber(charArray[2]);
    console.writeNum(code);
    // => 127854
}
